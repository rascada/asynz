{
  "name": "asynz",
  "version": "0.1.1",
  "description": "Hipster way of load async script in the browser",
  "main": "dist/asynz.js",
  "scripts": {
    "test": "babel-node test.js | faucet",
    "watch": "babel asynz.js --presets es2015 --out-file dist/asynz.js",
    "browser": "browserify test.js -o test_bundle.js -t [ babelify --presets [ es2015 ] ]",
    "test:server": "python -m SimpleHTTPServer",
    "test:dev": "npm run test:server & watchify test.js -o test_bundle.js -t babelify -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zzarcon/asynz.git"
  },
  "author": "zzarcon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zzarcon/asynz/issues"
  },
  "keywords": [
    "async",
    "script",
    "load",
    "append"
  ],
  "homepage": "https://github.com/zzarcon/asynz#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "faucet": "0.0.1",
    "relaser": "^0.2.0",
    "tape": "^4.5.1",
    "watchify": "^3.7.0"
  }
}
